{
  "title": "SLA Events",
  "description": "SLA breach/success events. Tracks response and resolution time against targets.",
  "schema": "dwh",
  "table_type": "Fact Table",
  "columns": {
    "sla_event_id": {
      "type": "bigint",
      "desc": "Primary key"
    },
    "issue_id": {
      "type": "bigint",
      "desc": "FK \u2192 dim_issue_h.issue_id"
    },
    "priority_id": {
      "type": "bigint",
      "desc": "FK \u2192 dim_priority_h.priority_id (cast to bigint)"
    },
    "sla_type": {
      "type": "text",
      "desc": "SLA type (response_time, resolution_time)"
    },
    "breached": {
      "type": "boolean",
      "desc": "True if SLA was breached"
    },
    "elapsed_ms": {
      "type": "bigint",
      "desc": "Elapsed time in milliseconds"
    },
    "target_ms": {
      "type": "bigint",
      "desc": "SLA target in milliseconds"
    }
  }
}